# name: var-ref
# key: -
# type: command
# --
(let* ((buffer-content (buffer-substring-no-properties (point-min) (point-max)))
       (matches (sort (delete-dups (my/string-match-all "--[-a-zA-Z0-9]+" buffer-content))))
       (value (completing-read "var(...): " matches)))
  (insert (format "var(%s)" value)))